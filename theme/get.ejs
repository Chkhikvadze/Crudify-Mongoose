<table>
	<% properties.forEach(function(property){ %>
		<tr>
			<th><%= property %></th>
			<td>
				<% if(typeof resource[property] !== 'undefined'){ %>
					<%= resource[property] %>
				<% } %>
			</td>
		</tr>
	<% }); %>
	<tr>
		<th>Actions</th>
		<td><a href="edit">Edit</a></td>
	</tr>
</table>